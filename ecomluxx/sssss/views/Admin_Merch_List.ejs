

<!-- Sidebar -->
<%- include('./Component/Sidebar.ejs') %>
<!-- Sidebar End -->

<div class="main-panel">
	 <!-- Navbar -->
   <%- include('./Component/Header.ejs') %>
<%

%>
   <div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header d-flex card-header-primary">
                  <h4 class="card-title mt-2 mr-3">View Merch</h4>
                  <ul class="nav nav-tabs" data-tabs="tabs">
                    <!-- <li class="nav-item">
                      <a class="nav-link active" href="#profile" onclick="getMeChange(1)" data-toggle="tab">
                        1 <i class="material-icons">label_important</i> 
                        <div class="ripple-container"></div>
                      </a>
                    </li> -->
                   
                  </ul>
                  <p class="card-category">
                    <!-- Recording Information -->
                  </p>
                </div>
                <div class="card-body">
                    <div class="row" id="row_two" >
                        <div class="col-md-12 table-responsive">
                            <table class="table" id="example_0" class="table  " style="width:100%; border: none;" >
                                <thead>
                                    <tr>
                                        <th> Sn. </th>
                                        <th>Image</th>
                                        <th>Title</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                  <%
                                  getMerchData.map((element, index)=> {
                                  %>
                                  <tr>
                                    <td> <%= index+1 %> </td>
                                    <td><img src="<%= element.image[0].src%>" height="50" alt=""></td>
                                    <td><%= element.title%></td>
                                    <td>
                                      <%
                                      let sta;
                                      let clas;
                                        if(element.Published == 0){
                                           sta = "Draft";
                                           clas = "btn btn-primary"
                                        }else if(element.Published == 1){
                                          sta = "Live";
                                          clas = "btn btn-success"
                                        }else if(element.Published == 2){
                                          sta = "Rejected";
                                          clas = "btn btn-danger"
                                        }
                                      %>
                                      <button class="<%=clas%>" > <%=sta%> </button>
                                    </td>
                                    <td> 
                                     <a href="/artist/merch/<%= element.productId%>" class="btn btn-primary" >

                                      View 
                                      </a>
                                    </td>
                                  </tr>
                                  <%
                                  })
                                  %>
                                </tbody>
                            </table>
                            <!-- <%= JSON.stringify(getMerchData)%> -->
                        </div>
                    </div>
                </div>
              </div>
            </div>
           
          </div> 


    </div>
  </div>
      <%- include('./Component/Footer.ejs') %>
 
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdn.ckeditor.com/4.16.0/standard/ckeditor.js"></script>

<script>
$(document).ready(function(){
    $('#example_0').DataTable();
    $(".dashboard_li_view_merch").addClass("active");
 

})

</script>